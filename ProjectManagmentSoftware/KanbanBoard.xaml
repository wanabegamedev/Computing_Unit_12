<Window x:Class="ProjectManagmentSoftware.KanbanBoard"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectManagmentSoftware"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        mc:Ignorable="d"
        ui:WindowHelper.UseModernWindowStyle="True"
        Title="KanbanBoard" Height="450" Width="800">
    <Grid x:Name="Kanban_Grid">
        <Button x:Name="LoadCreateCardWindow" Content="Create New Card" VerticalAlignment="Top" Click="LoadCreateCardWindow_Click" Background="Green" Height="30" Margin="0,394,0,0" Width="132"/>
    </Grid>
</Window>
